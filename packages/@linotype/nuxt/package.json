{
  "name": "@linotype/nuxt",
  "description": "Linotype Nuxt module",
  "version": "1.6.17",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linotype/linotype.git"
  },
  "author": {
    "name": "Yannick Armspach",
    "email": "linotype@ynk.es",
    "url": "https://ynk.es"
  },
  "maintainers": [
    {
      "name": "Yannick Armspach",
      "email": "linotype@ynk.es",
      "url": "https://ynk.es"
    }
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "directory": "dist"
  },
  "main": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/linotype/linotype#readme",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "scripts": {
    "update": "npx taze major",
    "prepack": "nuxt-module-build build",
    "build": "nuxt-module-build build",
    "dev": "npx nuxt dev playground",
    "play": "npx chokidar \"./../../../packages/@linotype/nuxt/src/**/*\" --initial -c \"export NODE_TLS_REJECT_UNAUTHORIZED=0 && npx kill-port 3000 && npx nx prepack @linotype/nuxt && npx node-notifier-cli notify -t 'linotype' -m 'Reloded' -s Glass -x 0 & npx nx dev playground-frontend\"",
    "dev:build": "npx nuxt build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt prepare playground",
    "release:current": "npm run lint && npm run prepack && npm publish --access public",
    "release:patch": "yarn version patch && yarn release:current",
    "release:minor": "yarn version minor && yarn release:current",
    "release:major": "yarn version major && yarn release:current",
    "lint": "eslint .",
    "test": "vitest run",
    "test:play": "npx chokidar \"./../../../packages/@linotype/nuxt/src/**/*\" --initial -c \"export NODE_TLS_REJECT_UNAUTHORIZED=0 && npx kill-port 3000 && npx nx prepack @linotype/nuxt && npx nx build playground-frontend && npx nx start playground-frontend\"",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2",
    "@unhead/vue": "^1.8.9",
    "h3-compression": "^0.3.2",
    "sitemap": "^7.1.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.9.0",
    "@types/node": "^20.10.5",
    "changelogen": "^0.5.5",
    "defu": "^6.1.3",
    "eslint": "^8.56.0",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.8.2",
    "stream": "^0.0.2",
    "vite": "^5.0.10",
    "vite-plugin-glob": "^0.3.2",
    "vitest": "^1.1.0",
    "vue": "^3.3.13"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
